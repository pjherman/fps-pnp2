    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "devDependencies": {
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
